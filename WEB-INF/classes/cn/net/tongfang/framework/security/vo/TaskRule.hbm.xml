<?xml version='1.0' ?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.net.tongfang.framework.security.vo.TaskRule" table="Task_Rule" >
        <id name="id">
            <column name="id" sql-type="nvarchar" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="nvarchar"/>
        </property>
        <property name="tablename">
            <column name="tablename" sql-type="nvarchar" not-null="true"/>
        </property>
        <property name="col">
            <column name="col" sql-type="nvarchar" not-null="true"/>
        </property>
        <property name="days">
            <column name="days" sql-type="int" not-null="true"/>
        </property>
        <property name="optdate">
            <column name="optdate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="msg">
            <column name="msg" sql-type="nvarchar" not-null="true"/>
        </property>
        <property name="wherestr">
            <column name="wherestr" sql-type="nvarchar" not-null="true"/>
        </property>
        <property name="tableidname">
            <column name="tableidname" sql-type="nvarchar"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar" length="10"/>
        </property>
        <property name="rulestr">
            <column name="rulestr" sql-type="varchar" length="500"/>
        </property>
        <property name="teltable">
            <column name="teltable" sql-type="varchar" length="50"/>
        </property>
        <property name="teljoinstr">
            <column name="teljoinstr" sql-type="varchar" length="50"/>
        </property>
        <property name="telcol">
            <column name="telcol" sql-type="varchar" length="50"/>
        </property>
        <property name="idtype">
            <column name="idtype" sql-type="varchar" length="50"/>
        </property>
        <property name="inputpage">
            <column name="inputpage" sql-type="varchar"/>
        </property>
        <property name="parent">
            <column name="parent" sql-type="varchar" length="50"/>
        </property>
        <property name="ord">
            <column name="ord" sql-type="int"/>
        </property>
        <property name="taskdatecol">
            <column name="taskdatecol" sql-type="nvarchar" length="50"/>
        </property>
    </class>
</hibernate-mapping>